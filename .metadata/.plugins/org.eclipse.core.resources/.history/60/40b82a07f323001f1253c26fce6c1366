package javase.core;

public class ClassInfo {

	// constructor

	// static final

	// instance variables
	// instance block
	// instance methods

	// inner class
}

class ClassInfoDetails {

	// constructor

	// static final

	// instance variables
	int x = 10;
	final int y = 20;

	// instance block
	// instance methods
	public void display() {
		int x = 10;
		final int y = 20; //Compile Time Constant (Compiler knows the value)
	}

	// inner class
	
	
	//main method 
	public static void main(String args[]) {
		System.out.println("main()");
	}
}
