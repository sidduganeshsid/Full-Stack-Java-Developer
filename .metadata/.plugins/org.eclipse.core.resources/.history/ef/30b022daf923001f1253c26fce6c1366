package javase.core;

class Test
{
	int a,b,c;
	
	//task will be brought into stack area for execution.
	void add()
	{
		a=1;
		b=2;
		c=a+b;
		System.out.println(c);
	}
}

public class All {

	public static void main(String[] args) {
		System.out.println("main() method is called");
		Test t = new Test();
		t.add();
		

	}

}
