package javase.oops;

class Father
{
	static float money = 3_00_000f;
}
class Son extends Father{
    static float money;
	
	static
    {
		money = Father.money;
	}

}


public class Inheritance {

	@SuppressWarnings("static-access")
	public static void main(String[] args) {
		
//		Son s = new Son();
		System.out.println(Son.money);
		
	}

}
